{
  "name": "chika-component",
  "version": "0.1.0",
  "description": "A set of react-component, IQ3 でもまかせなさいー！",
  "main": "lib",
  "module": "es",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "nino go -e ./src/go",
    "test": "nino test",
    "build": "tsc && nino koei -c ./scripts/nino.koei",
    "compile": "tsc && nino compile",
    "codecov": "nino test -d",
    "dev": "nino koei -w-d",
    "lint": "tslint -c tslint.json 'src/**/*.ts[x]'",
    "prettier": "nino prettier",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm config set registry=https://registry.npm.taobao.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish",
    "deploy": "npm run build && gh-pages -d dist -t"
  },
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "dependencies": {
    "antd-mobile": "^2.2.13",
    "leaflet": "^1.5.1",
    "nino-cli": "^0.8.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/leaflet": "^1.4.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "date-fns": "^2.0.0-alpha.32",
    "dekko": "^0.2.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "react-router-dom": "^5.0.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "license": "MIT",
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
